  <xs:element name="changePasscodeRequest">
    <xs:complexType>
      <xs:attribute name="brandCode" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The brand code associated with this request.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="identityProviderId" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity provider ID. Use "idmauth" for the
					default identity provider. Inbound SSO will specify the remote
					identity provider's ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="guid" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The globally unique identifier (GUID) for the
					identity principal.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="passcode" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>The current passcode. Either the current passcode
					or current password must be supplied.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="password" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>The current password. Either the current passcode
					or current password must be supplied.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="newPasscode" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The new passcode.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="source" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The LAN ID or login ID of the person or entity
					requesting the operation. Use "SELF" when the requestor's own user
					record is the subject of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
