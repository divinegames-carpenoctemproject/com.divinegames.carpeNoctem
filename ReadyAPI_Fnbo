    <xs:complexType>
      <xs:attribute name="result" type="tn:validateDirectConnectResultType" use="required">
        <xs:annotation>
          <xs:documentation>The result of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="identityPrincipalToken" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>A JWT token for the validated identity principal.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="tokenExpirationTimestamp" type="xs:dateTime" use="optional">
        <xs:annotation>
          <xs:documentation>The date and time the user token will expire. The
					token will not be valid on or after this time.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
