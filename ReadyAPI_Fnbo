      <xs:annotation>
        <xs:documentation>Service provider ID</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="description" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Description of the service principal</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="permission" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Permission assigned to the service principal</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="company" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Service principal company</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
