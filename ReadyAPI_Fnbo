  <xs:element name="validateDirectConnectRequest">
    <xs:complexType>
      <xs:attribute name="identityPrincipalName" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity principal name.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="password" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The password to be validated.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="productIdentifier" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The Intuit product identifier.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="loginTimestamp" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation>The date and time of the login.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="organization" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The organization name as provided to Intuit.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bankIdentifier" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The bank identifier.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="partnerReferenceNumber" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>B2B partner reference number for logging.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="clientUID" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>Intuit Client UID used for 2nd factor of
					authentication.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
