  <con:testStep type="groovy" name="randomPassword" id="bd74f58a-3e4d-4918-ad24-265eecedf892">
    <con:settings/>
    <con:config>
      <script>import org.apache.commons.lang.RandomStringUtils
/*
 * Generate random password 
 */
String charsetUpper = (('A'..'Z')).join()
String charsetLower = (('a'..'z')).join()
String intsetDigit = ('0'..'9').join()
String upperCase = RandomStringUtils.random(1,charsetUpper.toCharArray())
String lowerCase = RandomStringUtils.random(6,charsetLower.toCharArray())
String digit = RandomStringUtils.random(1,intsetDigit.toCharArray())
String randomPasscode = RandomStringUtils.random(4,intsetDigit.toCharArray())
String randomPassword = upperCase+lowerCase+digit
log.info ('Random password is: '+ randomPassword)
testRunner.testCase.setPropertyValue('RandomPassword', randomPassword.trim())
testRunner.testCase.setPropertyValue('RandomPasscode', randomPasscode.trim())</script>
    </con:config>
  </con:testStep>
