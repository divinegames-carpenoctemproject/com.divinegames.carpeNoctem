  <xs:element name="resetPasswordRequest">
    <xs:complexType>
      <xs:attribute name="brandCode" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The brand code associated with this request.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="identityProviderId" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity provider ID. Use "idmauth" for the
					default identity provider. Inbound SSO will specify the remote
					identity provider's ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="guid" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The globally unique identifier (GUID) for the
					identity principal.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="mailed" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>True if the password was mailed via the postal
					service in a sealed envelope. This will permit a longer validity
					period for the temporary password.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="source" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The LAN ID or login ID of the person or entity
					requesting the operation. Use "SELF" when the requestor's own user
					record is the subject of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="resetPasswordResponse">
    <xs:complexType>
      <xs:attribute name="temporaryPassword" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>A temporary password that must be changed before
					it expires and before the next authentication is completed.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="result" type="tn:simpleOperationResultType" use="required">
        <xs:annotation>
          <xs:documentation>The result of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="changePasswordRequest">
    <xs:complexType>
      <xs:attribute name="brandCode" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The brand code associated with this request.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="identityProviderId" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity provider ID. Use "idmauth" for the
					default identity provider. Inbound SSO will specify the remote
					identity provider's ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="guid" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The globally unique identifier (GUID) for the
					identity principal.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="fromPassword" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The old password. The correct old password must
					be supplied when this is a voluntary password change. When a
					password change is required due to expired or temporary password,
					then this attribute may be left blank.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="toPassword" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The new password. The new password must comply
					with the password strength requirements of the system.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="source" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The LAN ID or login ID of the person or entity
					requesting the operation. Use "SELF" when the requestor's own user
					record is the subject of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="changePasswordResponse">
    <xs:complexType>
      <xs:attribute name="result" type="tn:changePasswordResultType" use="required">
        <xs:annotation>
          <xs:documentation>The result of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
