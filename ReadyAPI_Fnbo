      <xs:annotation>
        <xs:documentation>The question text.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="answer" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The answer to the question supplied by the user.
				This value is masked in search results.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="questionOptionsType">
