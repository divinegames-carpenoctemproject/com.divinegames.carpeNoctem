  <xs:complexType name="federationType">
    <xs:attribute name="serviceProviderId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The service provider ID.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="identityProviderId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The identity provider ID. Use "idmauth" for the
				default identity provider. Inbound SSO will specify the remote
				identity provider's ID.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="priority" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>The priority of this federation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="authenticationUri" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Then authentication URI for this federation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="authenticationType" type="tn:authenticationTypeType" use="required">
      <xs:annotation>
        <xs:documentation>The authentication type used for this federation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="otpFromEmail" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The from email address for OTP</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="otpFromName" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The from name for OTP</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="identityPrincipalType">
    <xs:sequence>
      <xs:element name="question" type="tn:questionType" maxOccurs="unbounded" minOccurs="0"/>
      <xs:element name="departmentNumber" type="xs:string" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Employee department number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="employeeType" type="xs:string" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Employee type.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="prospectProduct" type="xs:string" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>One or more products and sub-products that 
				the prospect is applying for.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="name" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The identity principal name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="identityProviderId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The identity provider ID. Use "idmauth" for the
				default identity provider. Inbound SSO will specify the remote
				identity provider's ID.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="email" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The email address</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mobileNumber" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The mobile phone number</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mustChangeName" type="xs:boolean" use="required">
      <xs:annotation>
        <xs:documentation>This identity principal must change names during
				migration because the name already exists.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="personalMessage" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The personal message displayed during
				authentication.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="active" type="xs:boolean" use="required">
      <xs:annotation>
        <xs:documentation>True if this identity principal is active.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="locked" type="xs:boolean" use="required">
      <xs:annotation>
        <xs:documentation>True if this identity principal is locked.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="firstName" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>First name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="middleName" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Middle name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lastName" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Last name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lastLogin" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation>The date and time of the last successful login</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="description" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>A description that is used in special cases for
				noting the reason for temporary passwords or disabled identity
				principals.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="type" type="tn:principalTypeType" use="required">
      <xs:annotation>
        <xs:documentation>The type of identity principal.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="guid" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The GUID for this identity principal</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="uidCreateTimestamp" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation>The date and time that this identity principal was
				created.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="uidModifyTimestamp" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation>The date and time that this identity principal was
				last modified.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="notificationProvider" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The service provider that is used for the branding
				on notifications when a brand is not supplied.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="notificationOptOut" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>True if this user has opted out of optional
				notifications.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="passcodeLockedTime" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The date and time string when the passcode was
				locked due to failures.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mustChangePassword" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>True if the user must change the password during
				the next authentication.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="temporaryPasswordExpired" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>True if the temporary password is expired and must
				be reset by a CSR, administrator or by using Forgot Password.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="questionLockedTime" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The date and time string when the identity
				principal was locked due to failed security questions.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="agentIdentifier" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The agent bank of a partner identity principal.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="subagentIdentifier" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The subagent of a partner identity principal.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="employeeNumber" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Employee number.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="telephoneNumber" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Telephone number.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="affinityIdentifier" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The affinity identifier of an affinity partner identity principal.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prospectBankNumber" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>The bank number for a prospect. This field is always populated for
			prospects and is used to distinguish them from other users.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prospectServicePrincipal" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Populated with the service principal when the prospect has been 
			enrolled in CIXF.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="principalSearchResultEntryType">
    <xs:sequence>
      <xs:element name="identityPrincipal" type="tn:identityPrincipalType" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation>The identity principal that was found.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="servicePrincipal" type="tn:servicePrincipalType" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The service principals enrolled by the identity
					principal.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
