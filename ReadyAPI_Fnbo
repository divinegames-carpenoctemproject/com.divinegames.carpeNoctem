  </con:reporting><con:beforeRunScript>//Project path is stored in a variable for CI/CD purpose
project.setPropertyValue("ProjectPath", project.getProject().getPath().toString())
//Enable or disable test suites
project.testSuiteList.each { it.disabled = false }
project.testSuiteList.each { it.setPropertyValue( "TestReportRun", 'False')}

def testSuiteCount = project.getTestSuiteCount()
for(int i=0;i&lt;testSuiteCount;i++){
//Enable or diable test case property values
def testSuiteName = project.getTestSuiteAt(i).getName()
project.getTestSuiteByName(testSuiteName).testCaseList.each { it.setPropertyValue( "TestCaseStatus", '')}
project.getTestSuiteByName(testSuiteName).testCaseList.each { it.setPropertyValue( "TestCaseReportRun", 'False')}
}</con:beforeRunScript>
  <con:afterRunScript>//run report based on user request
def testProjectReportRun = context.expand( '${#Project#TestReportRun}' )
if(testProjectReportRun == 'True'){
  project.getTestSuiteByName("GenerateReport").getTestCaseByName("generateReport").run(new com.eviware.soapui.support.types.StringToObjectMap(), false)		
}

//set test case results to default values
def testSuiteCount = project.getTestSuiteCount()
for(int i=0;i&lt;testSuiteCount;i++){
def testSuiteName = project.getTestSuiteAt(i).getName()
project.getTestSuiteByName(testSuiteName).testCaseList.each { it.setPropertyValue( "TestCaseStatus", '')}
project.getTestSuiteByName(testSuiteName).testCaseList.each { it.setPropertyValue( "TestCaseReportRun", 'True')}
}</con:afterRunScript>
