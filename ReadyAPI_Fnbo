  <xs:element name="findAllByEmailAddressRequest">
    <xs:complexType>
      <xs:attribute name="identityProviderId" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity provider ID. Use "idmauth" for the
					default identity provider. Inbound SSO will specify the remote
					identity provider's ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="emailAddress" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The email address of the identity principal.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="source" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The LAN ID or login ID of the person or entity
					requesting the operation. Use "SELF" when the requestor's own user
					record is the subject of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="excludeMFA" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Exclude results from the OAAM system used for
					multi-factor authentication to improve performance and reduce
					load.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="findAllByEmailAddressResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="identityPrincipal" type="tn:identityPrincipalType" maxOccurs="unbounded" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The identity principals that were found.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
