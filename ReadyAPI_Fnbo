      <script>//errors variable
captureErrors = []

//Log assertion errors for direct connect
def validateDirectConnect = testRunner.testCase.testSteps["validateDirectConnect"]
for (assertion in validateDirectConnect.assertionList) {
	if (assertion.status.toString() == "FAIL") {
		testRunner.testCase.setPropertyValue("AssertionFailed", "Yes")
		for (e in assertion.errors)
		error = e.message
		def productIdentifier = context.expand( '${randomProductIdentifier#ProductIdentifier}' )
		captureErrors.add('validate direct connect failed for '+productIdentifier+' with reason: '+error)
	}
}

if(captureErrors.size()!=0){
	log.error (captureErrors)
	testRunner.testCase.setPropertyValue('ValidateErrors', captureErrors.toString())
}</script>
    </con:config>
