  <!--Direct Connect client types-->
  <xs:complexType name="directConnectClientType">
    <xs:attribute name="productIdentifier" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The Intuit product identifier for this client.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="enrollmentTimestamp" type="xs:dateTime" use="required">
      <xs:annotation>
        <xs:documentation>The date and time this client was first validated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="directConnectClientListType">
    <xs:sequence>
      <xs:element name="directConnectClient" type="tn:directConnectClientType" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Direct Connect clients for this service
					provider.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="serviceProviderId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The service provider ID.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="maximumClients" type="xs:integer" use="required">
      <xs:annotation>
        <xs:documentation>The maximum number of Client UIDs permitted for
				this identity principal and service provider combination.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="enrollmentExpirationTimestamp" type="xs:dateTime" use="required">
      <xs:annotation>
        <xs:documentation>The date and time the enrollment period will
				expire. No enrollments will be permitted on or after this time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <!--Operations-->
  <xs:element name="findRequest">
    <xs:complexType>
      <xs:attribute name="identityProviderId" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity provider ID. Use "idmauth" for the
					default identity provider. Inbound SSO will specify the remote
					identity provider's ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="name" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The identity principal name--typically called the
					user ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="source" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>The LAN ID or login ID of the person or entity
					requesting the operation. Use "SELF" when the requestor's own user
					record is the subject of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="excludeMFA" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Exclude results from the OAAM system used for
					multi-factor authentication to improve performance and reduce
					load.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="findResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="identityPrincipal" type="tn:identityPrincipalType" maxOccurs="1" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The identity principal that was found.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="result" type="tn:simpleFindResultType" use="required">
        <xs:annotation>
          <xs:documentation>The result of the operation.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
