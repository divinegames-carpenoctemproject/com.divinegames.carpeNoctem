if(testCaseReportRun == 'True'){
   testRunner.testCase.testSuite.project.getTestSuiteByName("GenerateReport").getTestCaseByName("generateReport").run(new com.eviware.soapui.support.types.StringToObjectMap(), false, false)		
}
//disable the test case
testRunner.testCase.disabled = true

//clear property values
testRunner.testCase.setPropertyValue( "Status", '')
testRunner.testCase.setPropertyValue( "DivTableErrorString", '')
testRunner.testCase.setPropertyValue( "DivFinalErrorString", '')
testRunner.testCase.setPropertyValue( "TestCaseReportRun", 'True')</con:tearDownScript>
