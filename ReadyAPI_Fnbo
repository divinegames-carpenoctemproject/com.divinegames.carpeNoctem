  <con:testStep type="groovy" name="reportCreation" id="db47b55c-f7bb-4aa2-b5e0-880a10cb8e2f">
    <con:settings/>
    <con:config>
      <script>import java.text.SimpleDateFormat
import java.util.concurrent.TimeUnit 
import java.net.*;

//-Adding inserted timestamp
def date = new Date()
def sdf = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss")
def reportTimeStamp = sdf.format(date).toString()
log.info('Report genereate time: '+reportTimeStamp)

def testSuiteCount = testRunner.testCase.testSuite.project.getTestSuiteCount()
finalErrorStrings = ''
int totalTestCaseRunPass = 0
int totalTestCaseRunFail = 0
int totalTestsRun = 0
int totalTestsPass = 0
int totalTestsFail = 0
long totalTimeTaken = 0

for (int j=0;j&lt;testSuiteCount;j++){
	String testSuiteName = testRunner.testCase.testSuite.project.getTestSuiteAt(j).getName()
	if(testSuiteName != 'GenerateReport') {

//get test case count
def testCases = testRunner.testCase.testSuite.project.getTestSuiteByName(testSuiteName)
def testCaseCount = testCases.getTestCaseCount()
log.info('Test case count: '+testCaseCount)

//loop through all test case to capture errors
for (int i=0; i&lt;testCaseCount; i++){
	def content = testCases.getTestCaseAt(i).getPropertyValue('Errors')
	testCaseRunPass = testCases.getTestCaseAt(i).getPropertyValue('TestCaseRunPass').toInteger()
	testCaseRunFail = testCases.getTestCaseAt(i).getPropertyValue('TestCaseRunFail').toInteger()
	totalTests = testCases.getTestCaseAt(i).getPropertyValue('TestsRun').toInteger() 
	totalPass = testCases.getTestCaseAt(i).getPropertyValue('TestsPass').toInteger()
	totalFail = testCases.getTestCaseAt(i).getPropertyValue('TestsFail').toInteger()
	totalTime = testCases.getTestCaseAt(i).getPropertyValue('TimeTaken').toInteger()
	totalTestCaseRunPass = testCaseRunPass+totalTestCaseRunPass
	totalTestCaseRunFail = testCaseRunFail+	totalTestCaseRunFail
	totalTestsRun = totalTests + totalTestsRun
	totalTestsPass = totalPass + totalTestsPass
	totalTestsFail = totalFail + totalTestsFail	
	totalTimeTaken = totalTime + totalTimeTaken
	if(content.isEmpty()==false){
		finalErrorStrings+=content+'\r\n'+'\r\n'
	}
}
	}
}
log.info finalErrorStrings
log.info('Total tests run: '+totalTestsRun)
log.info('Total test case pass: '+totalTestCaseRunPass)
log.info('Total test case fail: '+totalTestCaseRunFail)
if(totalTestCaseRunFail!=0){
	testRunner.testCase.setPropertyValue('EmailSubjectStatus','FAILED')
}
else{
	testRunner.testCase.setPropertyValue('EmailSubjectStatus','PASSED')	
}
//Total time taken
long minutes = TimeUnit.MILLISECONDS.toMinutes(totalTimeTaken) % 60; 
long seconds = TimeUnit.MILLISECONDS.toSeconds(totalTimeTaken) % 60;
long milliseconds = totalTimeTaken % 1000;

def totalTimeTakenString = String.format("%d Min %d Sec %d Ms", minutes, seconds, milliseconds)
log.info('Total runtime: '+totalTimeTakenString)


//create report tab content
def divFinalErrorString = context.expand( '${#TestCase#DivFinalErrorString}' )
def finalErrorString = 'Test Summary: \r\n'+'Timestamp: ' + reportTimeStamp +"\r\n"+
'Total number of tests cases run: ' + (totalTestCaseRunPass.toInteger()+totalTestCaseRunFail.toInteger()) +"\r\n"+
'Total number of tests cases pass: ' + totalTestCaseRunPass +"\r\n"+
'Total number of tests cases fail: ' + totalTestCaseRunFail +"\r\n"+  
'Total number of tests run: ' + totalTestsRun +"\r\n"+
'Total number of tests pass: ' + totalTestsPass +"\r\n"+
'Total number of tests fail: ' + totalTestsFail +"\r\n"+
'Total runtime: ' + totalTimeTakenString +"\r\n"+
"\r\n\r\n"+'Test Results: \r\n' +finalErrorStrings
testRunner.testCase.setPropertyValue( "DivFinalErrorString", finalErrorString)
log.error (finalErrorString)

//CREATE REPORT FOLDER
def reportPath = context.expand( '${#Project#ReportPath}' )
log.info reportPath
File dir = new File(reportPath+"\\"+"automationOkta");
if (!dir.exists()) {
dir.mkdirs()
}

//host name to send an email
String computerName = InetAddress.getLocalHost().getHostName();
testRunner.testCase.setPropertyValue('ComputerName', computerName)
log.info computerName

//check email address value
def mailAddress = context.expand( '${#Project#MailAddress}' )
if (mailAddress==''){
	testRunner.testCase.testSuite.project.setPropertyValue('MailAddress','DCEDeliveryTeam-Web1@fnni.com;cschmidt@fnni.com')
}</script>
