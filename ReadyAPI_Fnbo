	def errorString = 'Test Name: ' +testCaseName+'\r\n\t '+'Tests Run: '+ testsRun+'\t '+'Result: ' + testCaseStatus
	testRunner.testCase.setPropertyValue( "Errors", errorString)
	testRunner.testCase.setPropertyValue( "TestCaseRunPass", '1')
}
if(testCaseStatus=='FAIL') {
	def failErrors = context.expand( '${#TestCase#Errors}' )
	def reportSubHeading = 'Test Name: '+testCaseName+"\r\n"
	testRunner.testCase.setPropertyValue( "Errors", reportSubHeading+failErrors)
	testRunner.testCase.setPropertyValue( "TestCaseRunFail", '1')
}
//run report based on user request
