  <con:testStep type="groovy" name="assertions" id="25be1c63-89b0-46b8-b08d-8658b1567314">
    <con:settings/>
    <con:config>
      <script><![CDATA[import groovy.xml.Namespace
import groovy.util.XmlSlurper;
import groovy.util.slurpersupport.*;
import com.fnbo.automation.ldap.LDAP
import java.text.SimpleDateFormat
import com.eviware.soapui.support.XmlHolder

//-Get and set test case name
def testCaseName = testRunner.testCase.getName()
testRunner.testCase.setPropertyValue( "TestCaseName", testCaseName)
log.info ('Test case name: '+testCaseName)

//-Get and set test suite name
def testSuiteName = testRunner.testCase.testSuite.getName()
testRunner.testCase.setPropertyValue( "TestSuiteName", testSuiteName)
log.info ('Test suite name: '+testSuiteName)

//-Get and set unique identifier
def uniqueIdentifer = context.expand( '${dataSource#name}' )
testRunner.testCase.setPropertyValue( "UniqueIdentifier", uniqueIdentifer)
log.info ('Unique Identifer for '+testCaseName+' test case: '+uniqueIdentifer)

//capture errors and store into an array
captureErrors = []

//Log soap request assertion failures
def soapStep = testRunner.testCase.testSteps["findSoapRequest"]
for (assertion in soapStep.assertionList) {
	if (assertion.status.toString() == "FAIL") {
		testRunner.testCase.setPropertyValue("AssertionFailed", "Yes")
		for (e in assertion.errors)
		error = e.message
		captureErrors.add(error)
	}
}

/*
 * Source and target fields to be validated
 */
def sourceAttributeNames = ['cn','agentIdentifier', 'description', 'mail', 'employeeNumber', 'givenName', 'sn', 'orclGuid', 'subagentIdentifier'] 
def targetAttributeNames = ['name','agentIdentifier', 'description', 'email', 'employeeNumber', 'firstName', 'lastName', 'guid', 'subagentIdentifier']
log.info('Attributes count is '+sourceAttributeNames.size())

/*
 * Capture errors and log them in report
 */

void logAndStoreErrors(def columnName, def actualValue, def expectedValue) {
 if (actualValue != expectedValue) {	
  captureErrors.add('Field "<'+columnName+ '>" expected value is "<'+ expectedValue +'>"'
  + ' actual value is "<'+ actualValue+'>"'
   )
 }
}

/*
 * Get target response as xml from soap request and parse XML
 */
def assertionFailed = context.expand( '${#TestCase#AssertionFailed}' )
if(assertionFailed!="Yes"){
String requestResponse  = context.expand( '${findSoapRequest#Response}' )
def parseResponse = new XmlSlurper().parseText(requestResponse);
def groovyUtils = new com.eviware.soapui.support.GroovyUtils( context )
def holder = groovyUtils.getXmlHolder(requestResponse)

/*
 * Iterate through soap XML file and get node values
 * Get source data from LDAP connection
 */
def identityPrincipalTag = 'declare namespace pn=\'urn:fnbo.esb:/nppi/identity.oracle.ws/principal/schema/service/v3\'; //pn:findResponse[1]/pn:identityPrincipal[1]/@'
LDAP ldap = LDAP.newInstance('ldaps://tc.ovdq.fnni.com:7501','cn=users,o=fnn,c=us', 'cn=autotest,cn=users,o=fnn,c=us', 'Au+0Test')
def name = context.expand( '${dataSource#name}' )	
log.info('Running the test for name ': name)	
def results = ldap.search('(&(objectclass=person)(uid='+name+'))')


for (int i=0;i<sourceAttributeNames.size();i++ ){	 		
 		def targetData = context.expand( '${findSoapRequest#Response#'+identityPrincipalTag+targetAttributeNames[i]+'}' ).toLowerCase()-null	
 		def sourceData = results.getAt(sourceAttributeNames[i]).join(",").toLowerCase()-null
		logAndStoreErrors(sourceAttributeNames[i], targetData, sourceData) 		
}

//assertion for departmentNumber
def targetDeptNo= holder.getNodeValue("//pn:departmentNumber")?.toLowerCase()
def sourceDeptNo = results.getAt("departmentNumber").join(", ").toLowerCase()
if(!sourceDeptNo==null){
logAndStoreErrors('DepartmentNumber', targetDeptNo, sourceDeptNo)
}

//assertion for employeeType
def targetEmpType= holder.getNodeValues("//pn:employeeType")-null
log.info('Target EmployeeType value': targetEmpType.toString().isEmpty())
def sourceEmpTypeCount = results.getAt("employeeType").join(", ").toString()-null
log.info('Source EmployeeType value': sourceEmpTypeCount.toString().isEmpty())
def getSize= sourceEmpTypeCount.split(',').toList()
log.info('EmployeeType size ': getSize.size())

if(targetEmpType.toString().isEmpty() == true){
for (int j=0;j<getSize.size();j++){
	def sourceEmpTypeValue = targetEmpType[j].toString()?.toLowerCase()
	results.each{targetEmpTypeValues = "${it.employeeType}".toLowerCase()}
	results.each{targetEmpTypeValue = "${it.employeeType[j]}".toLowerCase()}
	if(targetEmpTypeValues.contains(sourceEmpTypeValue)== false){
		logAndStoreErrors('EmployeeType', targetEmpTypeValue-null, sourceEmpTypeValue-null)
	}	
}
}
}
//-logging final result in individual lines
String splitCaptureErrors = "";
for (int i = 0; i < captureErrors.size(); i++) {
 splitCaptureErrors = splitCaptureErrors + captureErrors[i] + '\r\n\t\t\t ' 
}

log.error splitCaptureErrors

//-Log result PASS or FAIL
def testsRun = context.expand( '${#TestCase#TestsRun}' ).toInteger()
def testsPass = context.expand( '${#TestCase#TestsPass}' ).toInteger()
def testsFail = context.expand( '${#TestCase#TestsFail}' ).toInteger()

if (splitCaptureErrors.isEmpty()){
	testsRun++
	testsPass++
	log.info("Pass")
	testRunner.testCase.setPropertyValue( "Status", "Pass")
	testRunner.testCase.setPropertyValue( "TestsRun", testsRun.toString())
	testRunner.testCase.setPropertyValue( "TestsPass", testsPass.toString())
}
if (!splitCaptureErrors.isEmpty()){
	testsRun++
	testsFail++
	log.info("Fail")
	testRunner.testCase.setPropertyValue( "Status", "Fail")
	testRunner.testCase.setPropertyValue( "TestsRun",  testsRun.toString())
	testRunner.testCase.setPropertyValue( "TestsFail", testsFail.toString())
}

def errorCreatedForLoop = context.expand('${#TestCase#ErrorString}')
def finalErrorString = errorCreatedForLoop + splitCaptureErrors 
testRunner.testCase.setPropertyValue("ErrorString", finalErrorString.trim())
log.error(finalErrorString)

//-Adding inserted timestamp
def date = new Date()
def sdf = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss:SSS")
testRunner.testCase.setPropertyValue( "ReportTimestamp", sdf.format(date).toString())
log.info(sdf.format(date).toString())

//- Assertion failed
def status = context.expand( '${#TestCase#Status}' )
log.info status
assert status == "Pass": "Data is not matched please view report/mail for details"]]></script>
