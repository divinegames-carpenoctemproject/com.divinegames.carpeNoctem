      <xs:element name="entry" type="tn:principalSearchResultEntryType" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The principals that were found.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="result" type="tn:principalSearchResultEnumType" use="required">
      <xs:annotation>
        <xs:documentation>The result of the search operation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="questionType">
